ACTOR Lucianeto : DoomPlayer
{
	+NORADIUSDMG
	Player.MaxHealth 50
	Player.MugShotMaxHealth 50
	Scale 0.9
	Speed 1.5
	Health 50
	Radius 16
	Height 56
	Mass 100
	PainChance 255
	Species "Player"
	DamageFactor "Friendly", 0
	DamageFactor "AFireF",0
	DamageFactor "Mina", 0
	Player.DisplayName "Lucianeto"
	Player.StartItem "LucianoPistol"
	Player.StartItem "Fist"
	Player.StartItem "Clip", 100
	Player.StartItem "Shell", 20
	Player.StartItem "LucianoID"
	Player.StartItem "LucianoArmorBonus"
	Player.WeaponSlot 1, Fist, Chainsaw2
	Player.WeaponSlot 2, LucianoPistol
	Player.WeaponSlot 3, LucianoSG, SuperShotgun2
	Player.WeaponSlot 4, ChainGun2
	Player.WeaponSlot 5, LucianoRL
	Player.WeaponSlot 6, PlasmaRifle2
	Player.WeaponSlot 7, BFG9000
	Player.WeaponSlot 8, LucianoBomba
	States
	{
		Death:
			PLAY H 0
			PLAY H 10 A_CustomMissile("MegaExplosion",1)
			// PLAY H 10 A_CustomMissile("MegaRocket",-1)
			PLAY I 10 A_PlayerScream
			PLAY J 10 A_NoBlocking
			PLAY KLM 10
			PLAY N -1
			Stop
		XDeath:
			PLAY O 0
			PLAY O 5 A_CustomMissile("MegaExplosion",1)
			//PLAY O 5 A_CustomMissile("MegaRocket",-1)
			PLAY P 5 A_XScream
			PLAY Q 5 A_NoBlocking
			PLAY RSTUV 5
			PLAY W -1
			Stop
	}
}
