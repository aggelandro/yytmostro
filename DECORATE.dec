ACTOR Shotgun2 : Shotgun {}
ACTOR ShotgunSpawner : CustomInventory replaces Shotgun
{
	Inventory.PickUpSound "$GOTSHOTGUN"
	States
	{
		Spawn:
			SHOT A -1
			Stop
		Pickup:
			TNT1 A 0 A_JumpIfInventory("LucianoID",1,"PickupLuciano")
			TNT1 A 0 A_JumpIfInventory("AggeID",1,"PickupAgge")
			TNT1 A 0 A_JumpIfInventory("RonID",1,"PickupAgge")
			TNT1 A 0 A_JumpIfInventory("zLayerID", 1, "PickupAgge")
			TNT1 A 0 A_JumpIfInventory("ThanuID",1,"PickupAgge")
			TNT1 A 0 A_JumpIfInventory("TerroID",1,"PickupTerro")
			Stop
		PickupLuciano:
			TNT1 A 0 A_JumpIfInventory("LucianoSG",1,"Spawn")
			TNT1 A 0 A_GiveInventory("LucianoSG",1)
			TNT1 A 0 A_PlaySound("misc/w_pkup")
			// TNT1 A 0 A_SpawnItemEX("ShotgunSpawner")
			Fail
		PickupAgge:
			TNT1 A 0 A_JumpIfInventory("Shotgun2",1,"Spawn")
			TNT1 A 0 A_GiveInventory("Shotgun2",1)
			TNT1 A 0 A_PlaySound("misc/w_pkup")
			// TNT1 A 0 A_SpawnItemEX("ShotgunSpawner")
			Fail
		PickupTerro:
			TNT1 A 0 A_JumpIfInventory("TerroShotgun",1,"Spawn")
			TNT1 A 0 A_GiveInventory("TerroShotgun",1)
			TNT1 A 0 A_PlaySound("misc/w_pkup")
			Fail
	}
}
