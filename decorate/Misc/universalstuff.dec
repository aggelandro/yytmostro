// Explosive Missile Junk
ACTOR Debris {
	Radius 3
	Scale 0.1
	+MISSILE
	+CLIENTSIDEONLY
	+DOOMBOUNCE
	+NOTELEPORT
	+FORCEXYBILLBOARD
	+THRUACTORS
	+DROPOFF
	BounceCount 3
	BounceFactor 0.4
	WallBounceFactor 0.3
	
	States {
		Spawn:
			SHRD ABCDEFGH 2
			Loop
			
		Death:
			TNT1 A 0 A_Jump(128,3,5)
			SHRD D 50
			SHRD D 5 A_FadeOut(0.10)
			Goto Death+2
			SHRD E 50
			SHRD E 5 A_FadeOut(0.10)
			Goto Death+4
			SHRD H 50
			SHRD H 5 A_FadeOut(0.10)
			Goto Death+6
	}
}

