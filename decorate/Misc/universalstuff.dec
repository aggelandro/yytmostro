ACTOR BlasterShot : FastProjectile {
  Radius 1
  Height 1
  Speed 50
  Damage (random(1,3)*3)
  Scale 0.7
  Projectile
  -RANDOMIZE
  RenderStyle Add
  Alpha 0.75
  Decal "BulletChipRED"
	SeeSound "enforcer/fire"
  DeathSound "blaster/hit"
	
  States {
		Spawn:
			LASR A 2 Bright
			Loop
			
		Death:
			TNT1 A 0 A_SpawnItem("ShotSmoke")
			SPRK ABCDEFGHIJKLMNOPQRSTUVW 1 Bright
			Stop
  }
}

