ACTOR HellFire : ArchvileFire
{
  Translation "208:231=192:207","248:249=192:207","160:167=192:207"
  DamageType "AfireF"
  +PAINLESS
  +MOVEWITHSECTOR
  -SOLID
  +FORCERADIUSDMG
  -NOGRAVITY
  +NODAMAGETHRUST
  Gravity 1
  States
  {
  // fire is active for 492 frames
  Spawn:
    FIRE A 2 bright 
    FIRE BAB 2 bright 
    FIRE C 2 bright 
    FIRE BCBCDCDCDEDED 6 bright A_Explode(6,20,false,false)
	FIRE BCBCDCDCDEDED 6 bright A_Explode(6,20,false,false)
	FIRE BCBCDCDCDEDED 6 bright A_Explode(6,20,false,false)
    FIRE E 2 bright
    FIRE FGH 4 bright
    Stop
  }
}
