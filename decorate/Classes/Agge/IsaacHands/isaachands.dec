ACTOR BrimstoneBeam : FastProjectile {
	Health 9999
	Radius 15
	Height 30
	Damage 0
	Speed 200
	Species "Player"
	DeathSound "weapons/plasmax"
	MissileType "BrimstoneTrail"
	MissileHeight 8
	Scale 0.8
	RenderStyle Add
	Alpha 0.15
	
	+PAINLESS
	+RIPPER
	+CANNOTPUSH
	+NODAMAGETHRUST
	+DONTRIP
	+NOBLOOD
	+NOBLOCKMAP
	+NOGRAVITY
	+MTHRUSPECIES
	
	States {
	
		Spawn:
			TNT1 A 0 NoDelay ACS_NamedExecuteWithResult("showBrimstoneTranslation")
			TNT1 A 0 A_JumpIfInventory("LostContactGiver", 1, "SpawnShieldedBeam", AAPTR_TARGET)
			TNT1 A -1 A_Jump(256, "SpawnLoop")
			Wait
			
		SpawnShieldedBeam:
			TNT1 A 0 A_ChangeFlag("SHOOTABLE", TRUE)
			TNT1 A 0 A_ChangeFlag("NOBLOCKMAP", FALSE)
			TNT1 A -1 A_Jump(256, "SpawnLoop")
			Wait
			
		SpawnLoop:
			BRS1 ABCD 1
			Loop
			
		Death:
			TNT1 A 0 A_JumpIfInventory("RubberCementGiver", 1, "BounceWall", AAPTR_TARGET)
			TNT1 A 0 A_GiveInventory("LoopFlag", 1)
			TNT1 A 0 A_JumpIfInventory("TheParasiteGiver", 1, "SplitBeam", AAPTR_TARGET)
			Goto DeathEnd
			
		BounceWall:
			TNT1 A 0 A_JumpIf(Z <= FloorZ+5 || Z >= CeilingZ-5, "DeathEnd")
			TNT1 A 0 A_JumpIfInventory("LoopFlag", 1, "DeathEnd")
			TNT1 A 0 A_SpawnItemEx("PlayerDoppelganger", -5, 0, -15, 0, 0, 0, 180, SXF_NOCHECKPOSITION)
			TNT1 A 0 A_GiveInventory("LoopFlag", 1)
			TNT1 A 0 A_JumpIfInventory("TheParasiteGiver", 1, "SplitBeam", AAPTR_TARGET)
			Goto DeathEnd
		
		SplitBeam:
			TNT1 A 0 A_JumpIfInventory("LoopFlag", 2, "DeathEnd")
			TNT1 A 0 A_SpawnItemEx("PlayerDoppelganger", -5, 0, -15, 0, 0, 0, 90, SXF_NOCHECKPOSITION)
			TNT1 A 0 A_SpawnItemEx("PlayerDoppelganger", -5, 0, -15, 0, 0, 0, -90, SXF_NOCHECKPOSITION)
			TNT1 A 0 A_GiveInventory("LoopFlag", 1)
			Goto DeathEnd
			
		DeathEnd:
			TNT1 A 0 ACS_NamedExecuteWithResult("setBrimstoneDeathRender")
			BRD1 A 3
			TNT1 A 0 A_SpawnItemEx("BrimstoneGib", 0, 0, -10, random(-5, 5), random(-5, 5), 3, random(0, 359))
			BRD1 BCD 3
			Stop
		
	}
}

