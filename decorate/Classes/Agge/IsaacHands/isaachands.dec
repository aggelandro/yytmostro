			TNT1 A 0 A_FireCustomMissile("NormalTearProjectile", 0, FALSE, 15)
			TNT1 A 0 A_FireCustomMissile("NormalTearProjectile", 0, FALSE, -15)
			CFLM F -1 A_Jump(256, 8) //OFFSET
