Actor TNT2 : Inventory { inventory.amount 1	inventory.maxamount 1 }
Actor CoopModeOn : Inventory { inventory.amount 1	inventory.maxamount 1 }
Actor WhoopsIDied : Inventory {	inventory.amount 1 inventory.maxamount 1 }
Actor TNTCounter : Inventory { inventory.amount 1 inventory.maxamount 3 }
Actor TNTSoundDelay : PowerDamage {	Damagefactor "normal", 1.0 powerup.duration -2 }
ACTOR ThrowPowerBig : Ammo { inventory.amount 1 inventory.MaxAmount 150 ammo.backpackamount 0 ammo.backpackmaxamount 150 +IGNORESKILL }

ACTOR SmokeTrail
{
	+NOINTERACTION
	+DONTSPLASH
	+NOCLIP
   	+NOGRAVITY
	+NOBLOCKMAP
	+CLIENTSIDEONLY
	RenderStyle Add
	States
	{
		Spawn:
			SMOK ABCDEFGHIJKLMNOPQ 1
			Stop
	}
}

ACTOR SmokeTrailIn4OutSlow : SmokeTrail
{	
    VSpeed 1
	Alpha 0
	States
	{
		Spawn:
			SMOK A 0
			SMOK A 1 A_SetTranslucent(.125,1)
			SMOK A 1 A_SetTranslucent(.25,1)
			SMOK B 1 A_SetTranslucent(.375,1)
			SMOK B 1 A_SetTranslucent(.5,1)
			SMOK C 1 A_SetTranslucent(.625,1)
			SMOK C 1 A_SetTranslucent(.75,1)
			SMOK D 1 A_SetTranslucent(.875,1)
			SMOK D 1 A_SetTranslucent(1,1)
			SMOK EFGH 2
			SMOK IJKLMNOPQ 1
			Stop
	}
}

