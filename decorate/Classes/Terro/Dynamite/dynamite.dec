	TNT1 A 0 A_JumpIfInventory("RocketAmmo",1,"RaisingDynamite")	
	Loop
  RaisingDynamite:
    PTNT V 3 A_WeaponReady(WRF_NOBOB|WRF_NOFIRE|WRF_NOSWITCH)	
    PTNT W 3 A_WeaponReady(WRF_NOBOB|WRF_NOFIRE|WRF_NOSWITCH)	
