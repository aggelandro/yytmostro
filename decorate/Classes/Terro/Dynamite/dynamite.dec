	PTNT Q 1 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)	
	TNT1 A 0 A_PlaySound("TNT/Match",6)	
	PTNT RST 1 A_WeaponReady(WRF_NOFIRE|WRF_NOSWITCH)
